{
    "mocha.requires": [
        "ts-node/register/files"
    ],
    "mocha.files.glob": "test/**/*.ts",
    "cSpell.enableFiletypes": [
        "solidity"
    ],
    "cSpell.words": [
        "PREC",
        "Thrld"
    ],
    "solidity.formatter": "prettier",
    "[solidity]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}